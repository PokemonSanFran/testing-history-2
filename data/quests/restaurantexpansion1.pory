    giveitem(ITEM_TEA,1)
    msgbox(gText_Restaurantexpansion_WaitOnlyOne,,SPEAKER_ZOEY)
    msgbox(gText_Restaurantexpansion_OfCourse,,SPEAKER_FALKNER)
    msgbox(gText_Restaurantexpansion_IRegret,,SPEAKER_ZOEY,TAIL_THOUGHT,EMOTE_ANGRY)
    closemessage
    release
    end
}
