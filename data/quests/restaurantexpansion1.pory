script Quest_Restaurantexpansion_ProgressToReward_Script{
    call(Quest_Restaurantexpansion_CountProgress_Script)
    if(var(VAR_TEMP_0) == ALL_NURSE){
        questmenu(QUEST_MENU_SET_REWARD,QUEST_RESTAURANTEXPANSION1)
    }
    return
}

script Quest_Restaurantexpansion_HangPoster_Script{
    call(Quest_Restaurantexpansion_ProgressToReward_Script)
    fadescreen(FADE_TO_BLACK)
    setobjectxy(PLAYER,12,2)
    TeleportCamera(12,2)
    applymovement(PLAYER,Common_Movement_FaceLeft)
    applymovement(NURSE,Common_Movement_FaceRight)

    special(GetCurrentMap)

    switch(var(VAR_RESULT)){
        case MAP_SOMA_POKEMONCENTER_1F: call(Quest_Restaurantexpansion_SOMA_HangPosterTile_Script)
        case MAP_BERKELEY_POKEMONCENTER_1F: call(Quest_Restaurantexpansion_Berkeley_HangPosterTile_Script)
        case MAP_HAIGHTASHBURY_POKEMONCENTER_1F: call(Quest_Restaurantexpansion_Haightashbury_HangPosterTile_Script)
        case MAP_MARIN_POKEMONCENTER_1F: call(Quest_Restaurantexpansion_Marin_HangPosterTile_Script)
        default: return
    }
    delay(8)
    fadescreen(FADE_FROM_BLACK)
    return
}

script Quest_Restaurantexpansion_Haightashbury_HangPosterTile_Script{
    subquestmenu(QUEST_MENU_CHECK_COMPLETE,QUEST_RESTAURANTEXPANSION1,SUB_QUEST_2)
    if (var(VAR_RESULT)){
    setmetatile(12,0,0x24D,TRUE)
    setmetatile(12,1,0x255,TRUE)
    special(DrawWholeMapView)
    }
    return
}

