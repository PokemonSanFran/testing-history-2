Quest_UnknownChuckA_HideLeader_Script::
	special GetNumberOfBadges
	compare VAR_CHUCK_STATE, BATTLE_1_COMPLETE
	goto_if_ne Quest_UnknownChuckA_HideLeader_Script_2
Quest_UnknownChuckA_HideLeader_Script_1:
	compare VAR_TEMP_0, 6
	goto_if_lt Quest_UnknownChuckA_HideLeader_Script_5
Quest_UnknownChuckA_HideLeader_Script_4:
	switch VAR_ALAMEDA_STATE
	case START_SURVIVAL_CHANCE, Quest_UnknownChuckA_HideLeader_Script_8
	case POST_SURVIVAL_CHANCE, Quest_UnknownChuckA_HideLeader_Script_8
	case WHY_ARE_YOU_HELPING_THEM, Quest_UnknownChuckA_HideLeader_Script_8
	case POST_EARTHQUAKE, Quest_UnknownChuckA_HideLeader_Script_8
	case START_EARTHQUAKE_RESCUE, Quest_UnknownChuckA_HideLeader_Script_8
	case POST_EARTHQUAKE_RESCUE, Quest_UnknownChuckA_HideLeader_Script_8
	case MEET_AT_PSFROUTE8, Quest_UnknownChuckA_HideLeader_Script_8
	return

Quest_UnknownChuckA_HideLeader_Script_2:
	setflag FLAG_TEMP_5
	goto Quest_UnknownChuckA_HideLeader_Script_1

Quest_UnknownChuckA_HideLeader_Script_5:
	setflag FLAG_TEMP_5
	goto Quest_UnknownChuckA_HideLeader_Script_4

Quest_UnknownChuckA_HideLeader_Script_8:
	setflag FLAG_TEMP_5
	return


