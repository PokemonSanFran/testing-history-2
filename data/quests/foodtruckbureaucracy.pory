    msgbox(gText_Foodtruckbureaucracy_ItsALongStoryBut,,SPEAKER_ZOEY,EMOTE_SWEAT)
    msgbox(gText_Foodtruckbureaucracy_AlrightIllAgree,,SPEAKER_WAITE)
    msgbox(gText_Foodtruckbureaucracy_OkayINeedTo,,SPEAKER_ZOEY)
