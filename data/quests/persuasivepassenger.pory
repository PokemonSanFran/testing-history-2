script Quest_Persuasivepassenger_Driver3_Event_Script{
    setvar(VAR_TEMP_1,5)
    lock
    if(!flag(FLAG_QUEST_PERSUASIVE_PASSENGER_PLAYER_KIDNAPPED)){
        msgbox(gText_Persuasivepassenger_HeyPlayer,,SPEAKER_UBER_DRIVER3)
        msgbox(gText_Persuasivepassenger_WhatTheHell,,SPEAKER_ZOEY)
    }else{
        msgbox(gText_Persuasivepassenger_WaitASecondHowDid,,SPEAKER_UBER_DRIVER3)
    }
