    waitse
}

script Quest_Persuasivepassenger_Uber_CEO_Dialogue{
    lock

        if (var(VAR_QUEST_PERSUASIVE_PASSENGER_STATE) != SAVED_BEFORE_UBER_CEO){
        if(defeated(TRAINER_UBER_CEO_RAID)){
            goto(Quest_Persuasivepassenger_PostRaid_Uber_CEO_Dialogue)
        }

        setvar(VAR_QUEST_PERSUASIVE_PASSENGER_STATE,SAVED_BEFORE_UBER_CEO)
        msgbox(gText_Persuasivepassenger_SoYoure,MSGBOX_NPC,SPEAKER_UBER_CEO)
        msgbox(gText_Persuasivepassenger_WhatsTheDeal,,SPEAKER_ZOEY,,EMOTE_ANGRY)
        msgbox(gText_Persuasivepassenger_WeDoThatMore,,SPEAKER_UBER_CEO)
        lockall
        closemessage
        startmenu_savescreen
    }
    goto(Quest_Persuasivepassenger_Uber_CEO_Battle_Script)
}

