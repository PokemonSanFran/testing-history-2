    msgbox(gText_Breaktheinternet_YouAreSomething,,SPEAKER_TWITTER_CEO)
    msgbox(gText_Breaktheinternet_WhereIs,,SPEAKER_ZOEY)
    msgbox(gText_Breaktheinternet_YourFriendWontBe,,SPEAKER_TWITTER_CEO)
