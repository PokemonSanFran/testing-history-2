script ImIn_StartHouseBreak_Script {
    lock
        addvar(VAR_MAUVILLE_CITY_STATE,1)

        fadescreen(FADE_TO_BLACK)
        call(ImIn_CutsceneSetup_OutsideHouse_Script)
        fadescreen(FADE_FROM_BLACK)

        applymovement(PLAYER,ImIn_ApproachHouse)
        waitmovement(PLAYER)

        applymovement(PSFELITE3,Common_Movement_FaceDown)

        setvar(VAR_0x8000, MUGSHOT_MARSHAL)
        callnative(DrawMugshot)
        msgbox(gText_ImIn_WereCertainNobody)

        setvar(VAR_0x8000, MUGSHOT_GRIMSLEY)
        callnative(DrawMugshot)
        msgbox(gText_ImIn_HasntLeftWorkEarly)

        //PSF TODO Figure out how to show that they're talking via walkie

        setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_ImIn_WereInPosition)
        closemessage
        callnative(ClearMugshot)

        applymovement(PSFNATIVERIVAL,ImIn_WalkAroundHouse)
        waitmovement(PSFNATIVERIVAL)

        applymovement(PSFELITE1,Common_Movement_FaceRight)
        applymovement(PSFELITE2,Common_Movement_FaceRight)
        applymovement(PSFELITE3,Common_Movement_FaceRight)
        applymovement(PSFELITE4,Common_Movement_FaceRight)
        applymovement(PLAYER,Common_Movement_FaceRight)
        waitmovement(PLAYER)

        setvar(VAR_0x8000, MUGSHOT_HUGH)
        callnative(DrawMugshot)
        msgbox(gText_ImIn_TheSideDoorWill)

        setvar(VAR_0x8000, MUGSHOT_SHAUNTAL)
        callnative(DrawMugshot)
        msgbox(gText_ImIn_LetsMoveThen)
        closemessage
        callnative(ClearMugshot)

        applymovement(PSFNATIVERIVAL,ImIn_WalkToSideDoor)
        waitmovement(PSFNATIVERIVAL)

        call(ImIn_ReleaseFromBall_Script)

        setvar(VAR_0x8000, MUGSHOT_HUGH)
        callnative(DrawMugshot)
        msgbox(gText_ImIn_BrickBreakSureComes)
        callnative(ClearMugshot)
        closemessage

        playse(SE_M_BRICK_BREAK)
        applymovement(PKMNA,ImIn_AttackHouse)
        waitse
        waitmovement(PKMNA)
        call(ImIn_DestroyWall_Script)

        applymovement(PKMNA,ImIn_WalkIntoHouseA)
        applymovement(PSFELITE1,ImIn_WalkIntoHouseB)
        applymovement(PSFELITE2,ImIn_WalkIntoHouseC)
        applymovement(PSFELITE3,ImIn_WalkIntoHouseD)
        applymovement(PSFELITE4,ImIn_WalkIntoHouseE)
        applymovement(PSFNATIVERIVAL,ImIn_WalkIntoHouseF)

        playse(SE_EXIT)
        playse(SE_EXIT)
        playse(SE_EXIT)
        playse(SE_EXIT)
        playse(SE_EXIT)
        playse(SE_EXIT)
        waitse
        waitmovement(PSFELITE1)

        removeobject(PKMNA)
        removeobject(PSFELITE1)
        removeobject(PSFELITE2)
        removeobject(PSFELITE3)
        removeobject(PSFELITE4)
        removeobject(PSFNATIVERIVAL)

        release
        end
}

