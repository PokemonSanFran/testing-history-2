        applymovement(PSFELITE1,Common_Movement_FaceRight)
        applymovement(PSFELITE3,Common_Movement_FaceRight)
        applymovement(PSFELITE4,Common_Movement_FaceLeft)
        applymovement(PSFELITE2,Common_Movement_FaceDown)
