        addobject(PSFELITE2)
        addobject(PSFELITE3)
        addobject(PSFELITE4)
        addobject(PSFNATIVERIVAL)

        setobjectxy(PSFELITE1,10,3)
        setobjectxy(PSFELITE3,10,4)
        setobjectxy(PSFELITE4,12,3)
        setobjectxy(PSFNATIVERIVAL,12,4)
        setobjectxy(PSFELITE2,11,2)

        setobjectxy(PLAYER,11,4)
