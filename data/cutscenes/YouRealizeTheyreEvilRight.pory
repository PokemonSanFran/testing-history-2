        lock
        msgbox(gText_YouRealizeTheyreEvilRight_ThisIsntWhere,,SPEAKER_ZOEY,TAIL_THOUGHT)
        closemessage
        applymovement(PLAYER,Common_Movement_WalkUp)
        waitmovement(PLAYER)
    }
    release
        end
}

