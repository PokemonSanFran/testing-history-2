        switch(var(VAR_RESULT)){
            case 0: goto(YouRealizeTheyreEvilRight_AskMoves_Script)
            default: goto(YouRealizeTheyreEvilRight_EndQuestions_Script)
        }
}

script YouRealizeTheyreEvilRight_AskMoves_Script {

