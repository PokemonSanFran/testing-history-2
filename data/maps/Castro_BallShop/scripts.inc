Castro_BallShop_MapScripts::
	map_script MAP_SCRIPT_ON_TRANSITION, Castro_BallShop_MapScripts_MAP_SCRIPT_ON_TRANSITION
	.byte 0

Castro_BallShop_MapScripts_MAP_SCRIPT_ON_TRANSITION:
	goto Castro_BallShop_MapScripts_OnTransition
	return


Castro_BallShop_InteractDudley_Event_Script::
	lock
	goto_if_set FLAG_TIMELINE_FALSE, Castro_BallShop_InteractDudley_Event_Script_2
	switch VAR_MASK_OFF_STATE
	case 1, Castro_BallShop_InteractDudley_Event_Script_10
	case 8, Castro_BallShop_InteractDudley_Event_Script_11
	call MaskOff_QuestD_Dialogue
Castro_BallShop_InteractDudley_Event_Script_1:
	release
	end

Castro_BallShop_InteractDudley_Event_Script_2:
	switch VAR_MANHUNT_STATE
	case NOT_ASSIGNED, Castro_BallShop_InteractDudley_Event_Script_6
	case GOT_MANHUNT_CLUE_2, Castro_BallShop_InteractDudley_Event_Script_7
	msgbox Castro_BallShop_InteractDudley_Event_Script_Text_0, MSGBOX_AUTOCLOSE
	goto Castro_BallShop_InteractDudley_Event_Script_1

Castro_BallShop_InteractDudley_Event_Script_6:
	return

Castro_BallShop_InteractDudley_Event_Script_7:
	call Manhunt_Dudley_Battle_Script
	goto Castro_BallShop_InteractDudley_Event_Script_1

Castro_BallShop_InteractDudley_Event_Script_10:
	return

Castro_BallShop_InteractDudley_Event_Script_11:
	return


Castro_BallShop_MapScripts_OnTransition::
	setflag FLAG_TEMP_2
	return


Castro_BallShop_InteractDudley_Event_Script_Text_0:
	.string "...$"
