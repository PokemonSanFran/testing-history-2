mapscripts Psfbadguy1Tower_Elevator_MapScripts {}

script Psfbadguy1Tower_ElevatorAttendant_Script {
    lock
    faceplayer
    setvar(VAR_0x8004,0)
    if (flag(FLAG_TEMP_2)){
        call(Psfbadguy1TowerElevator_EventScript_SetFloor)
    }
    copyvar(VAR_0x8006,VAR_DEPT_STORE_FLOOR)
	special(ShowDeptStoreElevatorFloorSelect)
    message(gText_WhichFloorWouldYouLike)
    waitmessage
    setvar(VAR_0x8004,0)
    specialvar(VAR_RESULT,GetDeptStoreDefaultFloorChoice)
    switch(var(VAR_RESULT))
       {
        case 0: goto(Psfbadguy1TowerElevator_EventScript_ChooseFloorFrom5th )
        case 1: goto(Psfbadguy1TowerElevator_EventScript_ChooseFloorFrom4th )
        case 2: goto(Psfbadguy1TowerElevator_EventScript_ChooseFloorFrom3rd)
        case 3: goto(Psfbadguy1TowerElevator_EventScript_ChooseFloorFrom2nd )
        case 4: goto(Psfbadguy1TowerElevator_EventScript_ChooseFloorFrom1st )
        }
    end
}

script Psfbadguy1TowerElevator_EventScript_ChooseFloorFrom5th{ 
    multichoicedefault(0,0,MULTI_FLOORS,0,FALSE)
    goto(Psfbadguy1TowerElevator_EventScript_ChooseFloor)
    end
}
