script Psfbadguy1TowerElevator_EventScript_ChooseFloor{
    switch(var(VAR_RESULT)) {
        //case 0: goto(Psfbadguy1TowerElevator_EventScript_5thFloor)
        //case 1: goto(Psfbadguy1TowerElevator_EventScript_4thFloor)
        //case 2: goto(Psfbadguy1TowerElevator_EventScript_3rdFloor)
        //case 3: goto(Psfbadguy1TowerElevator_EventScript_2ndFloor)
        case 4: goto(Psfbadguy1TowerElevator_EventScript_1stFloor)
        //case 5: goto(Psfbadguy1TowerElevator_EventScript_ExitFloorSelect)
        case(MULTI_B_PRESSED): goto(Psfbadguy1TowerElevator_EventScript_ExitFloorSelect)
    }
        end
}

script Psfbadguy1TowerElevator_EventScript_1stFloor{
    setvar(VAR_0x8006,DEPT_STORE_FLOORNUM_1F)
    setdynamicwarp(MAP_LILYCOVE_CITY_DEPARTMENT_STORE_1F,255,2,1)
    if (var(VAR_DEPT_STORE_FLOOR) == DEPT_STORE_FLOORNUM_1F)    {
        goto(Psfbadguy1TowerElevator_EventScript_ExitFloorSelect)
    }
    call(Psfbadguy1TowerElevator_EventScript_MoveElevator)
	setvar(VAR_DEPT_STORE_FLOOR,DEPT_STORE_FLOORNUM_1F)
	goto(Psfbadguy1TowerElevator_EventScript_ExitFloorSelect)
    end
}
