script Psfbadguy1TowerElevator_EventScript_ExitFloorSelect{
    special(CloseDeptStoreElevatorWindow)
    release
    end
}

script Psfbadguy1TowerElevator_EventScript_MoveElevator{
    special(CloseDeptStoreElevatorWindow)
    closemessage
    applymovement(VAR_LAST_TALKED,Common_Movement_WalkInPlaceFastestDown)
    waitmovement(0)
    waitse
    special(MoveElevator)
    waitstate
    setflag(FLAG_TEMP_2)
    return
}

script Psfbadguy1TowerElevator_EventScript_SetFloor{
    special(SetDeptStoreFloor)
    return
}

    
