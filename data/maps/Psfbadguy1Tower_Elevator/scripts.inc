	setvar VAR_0x8006, PSFBADGUY1TOWER_FLOORNUM_1F
	setdynamicwarp MAP_PSFBADGUY1TOWER_1F, 255, 8, 2
	compare VAR_DEPT_STORE_FLOOR, PSFBADGUY1TOWER_FLOORNUM_1F
