const PSFTRANSRIVAL = 1

mapscripts Psfcity10_MapScripts {
    MAP_SCRIPT_ON_TRANSITION {
        goto(Psfcity10_MapScripts_OnTransition)
    }

    MAP_SCRIPT_ON_FRAME_TABLE [
        VAR_ROUTE115_STATE, 1: IGuessWeShouldBeNiceNow_ExitCave_Script 
    ]
}

script Psfcity10_MapScripts_OnTransition {
    switch (var(VAR_ROUTE115_STATE)){
        case 0: goto(Psfcity10_HidePsftransrival_Script)
    }
}

script Psfcity10_HidePsftransrival_Script {
    setflag(FLAG_HIDE_PSFCITY10_PSFTRANSRIVAL)
    removeobject(PSFTRANSRIVAL)
}

