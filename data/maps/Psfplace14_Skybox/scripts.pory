mapscripts Psfplace14_Skybox_MapScripts {
    MAP_SCRIPT_ON_TRANSITION {
        goto(Psfplace14_Skybox_MapScripts_OnTransition)
    }
    MAP_SCRIPT_ON_FRAME_TABLE [
        VAR_STORYLINE_STATE, 28: WhyDidntYouRatMeOut_WatchPsftransrivalMatches
        VAR_STORYLINE_STATE, 33: WaitHeDidWhat_WatchOtherSemifinal_Dialogue 
    ]
}

script Psfplace14_Skybox_EmptySkybox_Script {
    setflag(FLAG_TEMP_2)
    setflag(FLAG_TEMP_1)
}

script Psfplace14_Skybox_MapScripts_OnTransition {
    switch (var(VAR_STORYLINE_STATE)){
        case 28: call(WaitHeDidWhat_HideObject_Script)
        case 29: call(WaitHeDidWhat_HideObject_Script)
        case 30: call(GroupStages_Skybox_ObjectSetup_Script)
        case 33: return
        case 34: call(WaitHeDidWhat_HideObject_Script)
        case 35: call(WaitHeDidWhat_HideObject_Script)
        default: call(Psfplace14_Skybox_EmptySkybox_Script)
    }
}
