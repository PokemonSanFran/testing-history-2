Salesforcetower_Leagueops_MapScripts::
	map_script MAP_SCRIPT_ON_TRANSITION, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION
	map_script MAP_SCRIPT_ON_FRAME_TABLE, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_FRAME_TABLE
	.byte 0

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION:
	switch VAR_SALESFORCETOWER_CONFERENCE_STATE
	case POST_WOW_YOURE_STRONG, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_2
	case START_THE_GANGS_ALL_HERE, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_3
	case POST_THE_GANGS_ALL_HERE, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_4
	case PRE_WELCOME_TO_THE_WAR_ROOM, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_5
	case POST_WELCOME_TO_THE_WAR_ROOM, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_6
	case PRE_YOU_REALIZE_WERE_EVIL_RIGHT, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_7
	case POST_YOU_REALIZE_WERE_EVIL_RIGHT, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_8
	case START_FALSE_TIMELINE, Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_9
	call Salesforcetower_Leagueops_EmptyConferenceRoomOffices
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_2:
	call TheGangsAllHere_ObjectSetup_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_3:
	call TheGangsAllHere_MidMission_Object_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_4:
	call TheGangsAllHere_ObjectCleanUp_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_5:
	call WelcomeToTheWarRoom_Object_Setup_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_6:
	call WelcomeToTheWarRoom_Object_Hide_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_7:
	call YouRealizeWereEvilRight_Object_Setup_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_8:
	call YouRealizeWereEvilRight_Object_Cleanup_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_TRANSITION_9:
	call CongratsYoureanAsshole_ObjectCleanUp_Script
	return

Salesforcetower_Leagueops_MapScripts_MAP_SCRIPT_ON_FRAME_TABLE:
	map_script_2 VAR_SALESFORCETOWER_CONFERENCE_STATE, PRE_WELCOME_TO_THE_WAR_ROOM, Salesforcetower_Leagueops_MovePlayerIntoPosition_Script
	map_script_2 VAR_SALESFORCETOWER_CONFERENCE_STATE, PRE_YOU_REALIZE_WERE_EVIL_RIGHT, Salesforcetower_Leagueops_MovePlayerIntoPosition_Script
	map_script_2 VAR_SALESFORCETOWER_CONFERENCE_STATE, POST_WOW_YOURE_STRONG, TheGangsAllHere_OakAppearStairs_Script
	.2byte 0


Salesforcetower_Leagueops_EmptyConferenceRoomOffices::
	setflag FLAG_TEMP_1
	setflag FLAG_TEMP_2
	setflag FLAG_TEMP_3
	setflag FLAG_TEMP_4
	setflag FLAG_TEMP_5
	setflag FLAG_TEMP_6
	return


Salesforcetower_Leagueops_MovePlayerIntoPosition_Script::
	lock
	getplayerxy VAR_TEMP_0, VAR_TEMP_1
	compare VAR_TEMP_0, 17
	goto_if_ne Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_2
Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_1:
	switch VAR_SALESFORCETOWER_CONFERENCE_STATE
	case PRE_WELCOME_TO_THE_WAR_ROOM, Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_5
	case PRE_YOU_REALIZE_WERE_EVIL_RIGHT, Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_6
	return

Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_2:
	fadescreen FADE_TO_BLACK
	setobjectxy OBJ_EVENT_ID_PLAYER, 17, 29
	TeleportCamera 17, 29
	fadescreen FADE_FROM_BLACK
	goto Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_1

Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_5:
	goto WelcomeToTheWarRoom_ExplainSitaution_Dialogue
	return

Salesforcetower_Leagueops_MovePlayerIntoPosition_Script_6:
	goto YouRealizeWereEvilRight_Open_Dialogue
	return


Salesforcetower_Leagueops_Oak_Event_Script::
	lock
	faceplayer
	switch VAR_STORYLINE_STATE
	case STORY_MORNING_OF_TIMELINE_SPLIT, Salesforcetower_Leagueops_Oak_Event_Script_3
	case STORY_START_FALSE_TIMELINE, Salesforcetower_Leagueops_Oak_Event_Script_4
	case STORY_1ST_TEAMROCKET_TAKEDOWN, Salesforcetower_Leagueops_Oak_Event_Script_5
	case STORY_2ND_TEAMROCKET_TAKEDOWN, Salesforcetower_Leagueops_Oak_Event_Script_6
Salesforcetower_Leagueops_Oak_Event_Script_1:
	release
	end

Salesforcetower_Leagueops_Oak_Event_Script_3:
	call CongratsYoureAnAsshole_Intro_Dialogue
	goto Salesforcetower_Leagueops_Oak_Event_Script_1

Salesforcetower_Leagueops_Oak_Event_Script_4:
	call LetsBurnThisMotherDown_Assignment_Dialogue
	goto Salesforcetower_Leagueops_Oak_Event_Script_1

Salesforcetower_Leagueops_Oak_Event_Script_5:
	call LetsBurnThisMotherDown_Assignment_Dialogue
	goto Salesforcetower_Leagueops_Oak_Event_Script_1

Salesforcetower_Leagueops_Oak_Event_Script_6:
	call LetsBurnThisMotherDown_Assignment_Dialogue
	goto Salesforcetower_Leagueops_Oak_Event_Script_1

