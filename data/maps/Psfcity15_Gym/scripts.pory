mapscripts Psfcity15_Gym_MapScripts {
    MAP_SCRIPT_ON_TRANSITION {
        goto(Psfcity15_Gym_MapScripts_OnTransition)
    }
    MAP_SCRIPT_ON_FRAME_TABLE[
    ]
}

script Psfcity15_Gym_MapScripts_OnTransition {
        switch (var (VAR_GYM_3_STATE)) { 
            case 0: return
            case 1: setflag(FLAG_TEMP_1)
            default: goto(EnterPsfleader3_StartBattle_ObjectSetup_Script)

        }
}

script Psfleader3_Event_Script {
    lockall
    setvar(VAR_0x8000, MUGSHOT_WHITNEY)
    callnative(DrawMugshot)
    switch (var (VAR_GYM_3_STATE)) {
    case 2:
        goto(EnterPsfleader3_MuseumArrangement_Dialogue)
    case 3:
        goto(EnterPsfleader3_StartBattle_Script)
    case 4:
        goto(EnterPsfleader3_PostBattle_Dialogue)
    case 5:
        goto(EnterPsfleader3_PostBattle_Dialogue2)
    }
}
