const PLAYER = OBJ_EVENT_ID_PLAYER
const PSFTRANSRIVAL = 1
const PSFGOODADMIN = 2
const PSFGOODBOSS = 3
const PSFNATIVERIVAL = 4
const PKMNA = 5
const PKMNB = 6

mapscripts Psfplace2_MapScripts {
    MAP_SCRIPT_ON_TRANSITION{
        goto(Psfplace2_MapScripts_OnTransition)
    }
    MAP_SCRIPT_ON_LOAD {
    }
    MAP_SCRIPT_ON_FRAME_TABLE [
        VAR_ROUTE118_STATE, 0: WhyAreYouHelpingThem_StartRaid
    ]
}

script Psfplace2_MapScripts_OnTransition {
    switch(var(VAR_ROUTE118_STATE)) {
        case 0: goto(WhyAreYouHelpingThem_HideObject_Script)
        case 1: goto(WhyAreYouHelpingThem_MoveObject_Script)
        case 2: goto(WhyAreYouHelpingThem_MoveObjectBoss_Script)
        case 4: goto(WhyAreYouHelpingThem_MoveObjectPsfgoodadmin_Script)
    }
}
