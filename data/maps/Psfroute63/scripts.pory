mapscripts Psfroute63_MapScripts {
    MAP_SCRIPT_ON_TRANSITION {
        goto(Psfroute63_MapScripts_OnTransition)
    }

    MAP_SCRIPT_ON_RESUME {
        goto(Psfroute63_MapScripts_OnResume)
    }
}

script Psfroute63_MapScripts_OnResume {
        if (defeated(TRAINER_GRUNT_AQUA_HIDEOUT_1)) {
            goto(HousingProtest_CleanUp_Script)
        }
}

script Psfroute63_MapScripts_OnTransition {
    switch (var (VAR_ROUTE101_STATE)) {
    case 0: goto(HousingProtest_ObjectSetUp_Script)
    case 1: goto(HousingProtest_PostSave_ObjectSetUp_Script)
    case 2: goto(HousingProtest_CleanUp_Landlord_Script)
    case 3: goto(HousingProtest_CleanUp_Landlord_Script)
    case 4: goto(SorryAboutMyFriends_ObjectCleanUp_Script)
    }
}
