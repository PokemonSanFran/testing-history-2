        playbgm(MUS_RG_ENCOUNTER_ROCKET,FALSE)
        setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_CongratsOnYourWin)
        goto(SorryAboutMyFriends_Continue_Dialogue)
}

