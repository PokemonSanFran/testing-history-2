script SorryAboutMyFriends_CheckBadge_Script {
        lockall

        if (flag(FLAG_BADGE04_GET)){
            goto(SorryAboutMyFriends_GiveTicket_Script)
        } else {
            goto(SorryAboutMyFriends_GetBadgeFirst_Dialogue)
        }
}

script SorryAboutMyFriends_GetBadgeFirst_Dialogue{
        lockall
        setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_WaitHoldOn)
        callnative(ClearMugshot)
        fadedefaultbgm
        releaseall
        end
}

script SorryAboutMyFriends_GiveTicket_Script {
        giveitem(ITEM_SS_TICKET,1)
        setvar(VAR_ROUTE101_STATE,5)
        addvar(VAR_FERRY_STATE,1)

        setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_IGiveYouALot)
        callnative(ClearMugshot)

        setvar(VAR_0x8000, MUGSHOT_HILDA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_IAppreciateTheGesture)
        callnative(ClearMugshot)

        setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_WantedYouToGoThere)
        callnative(ClearMugshot)

        setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_ExcellentPlaceToTrain)
        callnative(ClearMugshot)

        if (var(VAR_FAINTED_FOG_STATE) > 0){
            setvar(VAR_0x8000, MUGSHOT_HILDA)
                callnative(DrawMugshot)
                msgbox(gText_SorryAboutMyFriends_WaitForTheFog)
                callnative(ClearMugshot)
        }

    setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_FollowSomeMoreLeads)
        callnative(ClearMugshot)

        setvar(VAR_0x8000, MUGSHOT_HILDA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_AfterWeBattled)
        callnative(ClearMugshot)

        setvar(VAR_0x8000, MUGSHOT_ARIANA)
        callnative(DrawMugshot)
        msgbox(gText_SorryAboutMyFriends_ThankYouEnjoy)
        callnative(ClearMugshot)
        closemessage

        applymovement(ARIANA,SorryAboutMyFriends_WalkSouth)
        waitmovement(0)
        removeobject(ARIANA)
        fadedefaultbgm
        releaseall
        end
}

movement HousingProtest_WalkPlayer1Left {
    walk_left * 1
        step_end
}

movement HousingProtest_WalkPlayer2Left {
    walk_left * 2
        step_end
}

movement HousingProtest_WalkPlayer3Left {
    walk_left * 3
        step_end
}

movement HousingProtest_WalkPlayer4Left {
    walk_left * 4
        step_end
}

movement HousingProtest_ApproachKatell {
    walk_up * 4
        step_end
}

movement HousingProtest_DefendKatell {
    walk_left * 3
        face_down
        step_end
}

movement HousingProtest_StepAside {
    walk_left
        face_down
        step_end
}

movement HousingProtest_WalkSouthFromKatell {
    walk_down * 4
        step_end
}

movement HousingProtest_WalkAroundPlayer {
    walk_right * 2
        walk_down * 4
        step_end
}

movement SorryAboutMyFriends_WalkSouth {
    walk_down * 6
        step_end
}

