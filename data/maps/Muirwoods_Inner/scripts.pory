mapscripts Muirwoods_Inner_MapScripts {
    MAP_SCRIPT_ON_TRANSITION {
        goto(Muirwoods_Inner_MapScripts_OnTransition)
    }
    MAP_SCRIPT_ON_FRAME_TABLE [
        VAR_MUIRWOODS_STATE, 3: EnterTheMaster_ClearingIntro_Dialogue
        VAR_MUIRWOODS_STATE, 5: HaveYouSeenTheNews_AskHelp_Dialogue
    ]
}

script Muirwoods_Inner_MapScripts_OnTransition {
    switch (var(VAR_MUIRWOODS_STATE)) {
        case 3: goto(EnterTheMaster_Clearing_Object_SetUp_Script)
        case 5: return
        default: goto(EnterTheMaster_HideObject_Script)
    }
}

script HaveYouSeenTheNews_SolvePuzzles_Script {
    addvar(VAR_MUIRWOODS_STATE,1)
    msgbox(format("you've solved Siebold's tests. go back now."))
    closemessage
}

script Muirwoods_Siebold_Interact_Script {
    lock
    faceplayer
    switch (var(VAR_MUIRWOODS_STATE)){
        case 6: msgbox(gText_HaveYouSeenTheNews_SoundsLikeYouve,,SPEAKER_SIEBOLD)
        default: call(EnterTheMaster_ExplainRobots_Dialogue)
            }
        closemessage
        release
        end
} 
