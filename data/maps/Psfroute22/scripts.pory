mapscripts Psfroute22_MapScripts {
    MAP_SCRIPT_ON_TRANSITION {
        goto(Psfroute22_MapScripts_OnTransition)
    }
    MAP_SCRIPT_ON_LOAD {
        goto(Psfroute22_MapScripts_OnLoad)
    }
    MAP_SCRIPT_ON_FRAME_TABLE [
        VAR_ROUTE110_STATE, 1: TheStrikeStrikesBack_Intro_Dialogue
        VAR_ROUTE110_STATE, 3: VSGarbodor_Arrest_Script
        VAR_STORYLINE_STATE, 51: MaybeIFuckedUp_Dialogue
    ]
}

script Psfroute22_MapScripts_OnTransition {
    switch (var(VAR_ROUTE110_STATE)){
        case 0: call(TheStrikeStrikesBack_HideObject_Script)
        case 1: call(TheStrikeStrikesBack_ObjectSetup_Script)
        case 2: call(TheStrikeStrikesBack_HideObject_Script)
        case 3: call(VSGarbodor_Arrest_ObjectSetup_Script)
        case 4: call(TheStrikeStrikesBack_HideObject_Script)
    }

    if (var(VAR_STORYLINE_STATE) == 51){
        clearflag(FLAG_TEMP_6)
    }

}

script Psfroute22_MapScripts_OnLoad {
        if (var(VAR_STORYLINE_STATE) > 23){ //PSF TODO what is this variable supposed to be
            call(HeresHowThisIsGoingToGo_ConstructionCompleted_Script)
        }

        if (var(VAR_ROUTE110_STATE)) {
            call(TheStrikeStrikesBack_UnblockConstruction_Script)
        }
}
