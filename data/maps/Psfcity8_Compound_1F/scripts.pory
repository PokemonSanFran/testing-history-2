    setvar(VAR_0x8000, MUGSHOT_BIANCA)
    playbgm(MUS_ENCOUNTER_BRENDAN,TRUE)
    playse(SE_PIN)
	applymovement(PSFCHARA4,Common_Movement_ExclamationMark)
