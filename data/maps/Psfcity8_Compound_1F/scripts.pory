script Psfchara4Annoyed_Dialogue {
    setvar(VAR_0x8000, MUGSHOT_BIANCA)
    callnative(DrawMugshot)

    switch(var(VAR_TEMP_0)) {
            case 1:
                msgbox(gText_ImProbablyBetterThanYou_Psfchara4Annoy_Text2,MSGBOX_NPC)
